<div class="container">
<form>
  <div class="form-group">
    <label for="jobName">Job Name</label>
    <input type="text" class="form-control" id="jobName">
  </div>
  <div class="form-group">
    <label for="jobDescription">Job Description</label>
    <textarea class="form-control" id="jobDescription" rows="3"></textarea>
  </div>
  <div class="form-check">
  <input class="form-check-input" type="checkbox" value="html" id="htmlNeeded">
  <label class="form-check-label" for="htmlNeeded">
    Requires HTML
  </label>
  </div>
  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="css" id="cssNeeded">
    <label class="form-check-label" for="cssNeeded">
      Requires CSS
    </label>
  </div>
  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="js" id="jsNeeded">
    <label class="form-check-label" for="jsNeeded">
      Requires JavaScript
    </label>
  </div>
    <button type="submit" id="submit" class="btn btn-primary">Submit</button>
</form>

</div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/public/js/cliUser.js"></script>
<script src="/public/js/index.js"></script>